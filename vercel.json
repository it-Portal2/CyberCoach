{
  "version": 2,
    "functions": {
    "api/analyzeResume.js": {
      "memory": 1024,
      "maxDuration": 60
    }
  },
  "rewrites": [
        {
          "source": "/(.*)",
          "destination": "/index.html"
        }
      ],

  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ]
  
}